{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${config:idf.espIdfPath}/components/**",
            ],      
            "browse": {
                "path": [
                  "${config:idf.espIdfPath}/components",
                  "${config:idf.espIdfPathWin}/components",
                  "${workspaceFolder}"
                ],        
                "limitSymbolsToIncludedHeaders": false
            },
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}